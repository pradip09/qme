@font-face {
    font-family: 'harvestregular';
    src: url('../fonts/harvest_-webfont.eot');
    src: url('../fonts/harvest_-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/harvest_-webfont.woff') format('woff'),
         url('../fonts/harvest_-webfont.ttf') format('truetype'),
         url('../fonts/harvest_-webfont.svg#harvestregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'allerregular';
    src: url('../fonts/aller_rg-webfont.eot');
    src: url('../fonts/aller_rg-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/aller_rg-webfont.woff') format('woff'),
         url('../fonts/aller_rg-webfont.ttf') format('truetype'),
         url('../fonts/aller_rg-webfont.svg#allerregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'droid_sansregular';
    src: url('../fonts/droidsans-webfont.eot');
    src: url('../fonts/droidsans-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/droidsans-webfont.woff') format('woff'),
         url('../fonts/droidsans-webfont.ttf') format('truetype'),
         url('../fonts/droidsans-webfont.svg#droid_sansregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

/* Header */

body.inner_page { 	background: url(../images/innerwrapperbg.png) repeat-x #F7F7F7;	 }

#header { background: transparent; height: 106px; z-index: 100; position: relative; } 
#header_right{ float:right;} 
#logo { position: reletive; margin-left: 18px; margin-top: 2px; } 
.btnlogo { display: block; height: 101px; width: 101px; background: url(../images/logo.png) no-repeat;  background-position: left top; cursor: pointer; -webkit-font-smoothing: antialiased; text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.004); }
.btnlogo span.hover { display: block; height: 101px; width: 101px; text-align: center; font-size: 0px; background: url(../images/logo.png) no-repeat; background-position: left bottom; }

.nav ul { float: left; width: 100%; margin: 0 auto; list-style: none; }

.nav li { float: left; position: relative; font-family: Arial; font-weight: normal; margin: 0 9px 0 1px; padding: 0; display:list-item; }
.nav li a { color: #F7F7F7; display: inline-block; margin: 0 5px 0 0; padding: 12px 5px 10px 0; font-family: 'harvestregular', Arial;
font-size: 0.95em; letter-spacing: 1px; text-transform: uppercase; -webkit-transition-duration: .5s; -moz-transition-duration: .5s; -o-transition-duration: .5s;
-ms-transition-duration: .5s; transition-duration: .5s;  -webkit-font-smoothing: antialiased; text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.004); text-decoration:none; } 
.nav li a:hover,.nav li a.active1 { color: #FF9216; text-decoration: none; }
.nav span { color: inherit; font-size: inherit; padding-right: inherit; font-family: inherit; }
/* Header End */

